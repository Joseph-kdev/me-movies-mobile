{
  "expo": {
    "name": "me-movies-mobile",
    "slug": "me-movies-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Me-Movies.png",
    "scheme": "memoviesmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.memoviesmobile",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Me-Movies.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.memoviesmobile",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Me-Movies.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Me-Movies.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Buda.ttf",
            "./assets/fonts/Oxanium.ttf",
            "./assets/fonts/Poppins.ttf",
            "./assets/fonts/RubikDirt.ttf",
            "./assets/fonts/TiltNeon.ttf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "turboModules": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a370d28d-1c0c-4915-81f6-d74f4073885a"
      }
    }
  }
}
